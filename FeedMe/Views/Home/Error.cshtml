@{
	ViewBag.Title = "Error";
}

<div class="container">
	<div class="well register_form">
		<h2>Erro</h2>

		<div>@ViewBag.Message</div>
		@if ( @ViewBag.ReturnUrl != null )
  {
			<a href="@ViewBag.ReturnUrl">Clique aqui para voltar à página anterior.</a>
  } else
  {
			<a href="/">Clique aqui para voltar à página inicial.</a>
  }
	</div>
</div>
