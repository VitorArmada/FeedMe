@model FeedMe.Models.Receita

<strong>Classificação </strong>
@if (Model.averageClassificacao() == 0)
{
    <label>Sem Feed</label>
}
else
{
    for (int i = 0; i < Model.averageClassificacao(); i++ )
    {
        <i class="icon-star"></i>
    }
    <label>(@Html.DisplayFor(model => model.Classificacoes.Count) votos)</label>
}